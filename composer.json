{
    "name": "goksagun/rate-limit-bundle",
    "description": "Rate limit bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Burak Bolat",
            "email": "brkblt@gmail.com"
        }
    ],
    "keywords": [
        "rate-limit bundle"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "~2.7|~3.0|~4.0"
    },
    "require-dev": {
        "predis/predis": "^0.8|^1.1",
        "symfony/phpunit-bridge": "*"
    },
    "autoload": {
        "psr-4": { "Goksagun\\RateLimitBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
